{{#if lazyloadActive}}
  {{#if inlineImage}}<img src="{{unbound lazyload-src}}" data-src="{{unbound href}}" title="{{unbound data-title}}" class="lazyload {{unbound data-class}}"/>{{/if}}{{yield}}
{{else}}
  {{#if inlineImage}}<img src="{{unbound href}}" title="{{unbound data-title}}" class="{{unbound data-class}}"/>{{/if}}{{yield}}
{{/if}}